{
  "name": "redux-entity",
  "version": "1.0.0",
  "description": "small framework to reduce action name generation work for CRUD operation in Restful",
  "main": "index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ababakov/redux-entity.git"
  },
  "keywords": [
    "redux",
    "react",
    "entity",
    "crud",
    "rest"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ababakov/redux-entity/issues"
  },
  "homepage": "https://github.com/ababakov/redux-entity#readme",
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "eslint": "^7.11.0",
    "husky": "^4.3.0",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.2",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "query-string": "^6.13.1"
  }
}

{
  "name": "redux-entity",
  "version": "1.0.0",
  "description": "small framework to reduce action name generation work for CRUD operation in Restful",
  "main": "index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src/**/*.ts --fix",
    "build": "tsc",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn test && yarn run lint",
    "preversion": "yarn run lint",
    "version": "yarn run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ababakov/redux-entity.git"
  },
  "keywords": [
    "redux",
    "react",
    "entity",
    "crud",
    "rest"
  ],
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "author": "ababakov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ababakov/redux-entity/issues"
  },
  "homepage": "https://github.com/ababakov/redux-entity#readme",
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/eslint-plugin-tslint": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.1.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.1.2",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "query-string": "^6.13.1"
  }
}

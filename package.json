{
  "name": "redux-yeti",
  "version": "0.0.1",
  "description": "small framework to reduce action name generation work for CRUD operations in Restful",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc --build tsconfig.build.json",
    "clear": "rm -rf dist/",
    "prepare": "yarn run build",
    "lint": "eslint src/**/*.ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ababakov/redux-yeti.git"
  },
  "keywords": [
    "redux",
    "react",
    "entity",
    "crud",
    "rest"
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,ts,tsx}": [
      "prettier --write"
    ]
  },
  "author": "ababakov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ababakov/redux-yeti/issues"
  },
  "homepage": "https://github.com/ababakov/redux-yeti#readme",
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@types/react-redux": "^7.1.9",
    "@types/redux": "^3.6.0",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/eslint-plugin-tslint": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.1.0",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2",
    "ts-jest": "^26.1.2",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "query-string": "^6.13.1"
  }
}
